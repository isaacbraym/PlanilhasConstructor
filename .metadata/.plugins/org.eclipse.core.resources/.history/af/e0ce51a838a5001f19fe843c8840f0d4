package com.abnote.planilhas.estilos.estilos;

public enum CorEnum {
    VERMELHO_ESCURO(139, 0, 0),
    AZUL(0, 0, 255),
    VERDE(0, 128, 0),
    // Adicione outras cores conforme necess√°rio

    private final int r;
    private final int g;
    private final int b;

    CorEnum(int r, int g, int b) {
        this.r = r;
        this.g = g;
        this.b = b;
    }

    public int getR() {
        return r;
    }

    public int getG() {
        return g;
    }

    public int getB() {
        return b;
    }
}
