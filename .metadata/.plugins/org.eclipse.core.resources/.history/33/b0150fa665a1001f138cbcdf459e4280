import org.apache.poi.ss.usermodel.Workbook;

import java.util.List;

public interface IPlanilha {
    void criarPlanilha(String nomeSheet);
    void inserirDadosColuna(List<String> dados, int numeroColuna, int linhaInicial);
    void aplicarBoldStyle(int numeroColuna);
    Workbook obterWorkbook(); // Para obter o workbook e salvá-lo externamente, se necessário
}
